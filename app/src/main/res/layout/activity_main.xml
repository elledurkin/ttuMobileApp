<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="androidStudio.Harris.org.MainActivity">


    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/gridLayout"
        android:background="@drawable/ttu_sepia">

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton"
            android:layout_row="0"
            android:layout_column="0"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_height"
            android:layout_marginRight="25dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="18dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/admissions_icon2" />

        <ImageButton
            android:layout_width="67dp"
            android:layout_height="71dp"
            android:id="@+id/imageButton2"
            android:layout_row="0"
            android:layout_column="1"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:layout_marginTop="18dp"
            android:layout_marginRight="25dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/trophy_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton3"
            android:layout_row="0"
            android:layout_column="2"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:layout_marginTop="18dp"
            android:layout_marginRight="25dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/bus_icon_two" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton4"
            android:layout_row="0"
            android:layout_column="3"
            android:layout_marginTop="18dp"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/coures_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton5"
            android:layout_row="1"
            android:layout_column="0"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/dining_icon_two" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton9"
            android:layout_row="1"
            android:layout_column="1"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/directory_icon_2" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton10"
            android:layout_row="1"
            android:layout_column="2"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/emergency_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton11"
            android:layout_row="1"
            android:layout_column="3"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/calendar_icon_2" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton6"
            android:layout_row="2"
            android:layout_column="0"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/final_exam_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton12"
            android:layout_row="2"
            android:layout_column="1"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/hours_of_operation_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton13"
            android:layout_row="2"
            android:layout_column="2"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/library_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton14"
            android:layout_row="2"
            android:layout_column="3"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/map_icon3" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton7"
            android:layout_row="3"
            android:layout_column="0"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/doublet_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton15"
            android:layout_row="3"
            android:layout_column="1"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/news_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton16"
            android:layout_row="3"
            android:layout_column="2"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/photos_icon" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton17"
            android:layout_row="3"
            android:layout_column="3"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton8"
            android:layout_row="4"
            android:layout_column="0"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:layout_marginLeft="9dp"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/studentorg_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton18"
            android:layout_row="4"
            android:layout_column="1"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/bullhorn_icon" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:id="@+id/imageButton19"
            android:layout_row="4"
            android:layout_column="2"
            android:minHeight="@android:dimen/notification_large_icon_height"
            android:minWidth="@android:dimen/notification_large_icon_width"
            android:background="@drawable/videos_icon" />



    </GridLayout>


</RelativeLayout>
